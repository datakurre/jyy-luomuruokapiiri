Options +FollowSymLinks +MultiViews

MultiviewsMatch Any

RewriteEngine  on
# Korvaa seuraavan rivin /~datakurre/japary/ omalla polulla
RewriteBase    /~datakurre/japary/
RewriteCond    %{REQUEST_FILENAME}         -f
RewriteRule    ^(.+[^/]).php$               $1/                  [R]
RewriteRule    ^(.+[^/]).php/hallinta/?$    $1/?view=orderbook   [R]
RewriteRule    ^(.+[^/]).php/(tietosuojaseloste.pdf)$  $1-$2     [R,L]
RewriteRule    ^(.+[^/]).php/(.*\..{2,4})$  $2                   [R]